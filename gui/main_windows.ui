              <widget class="QTableWidget" name="tableWidget">
               <property name="styleSheet">
                <string notr="true">/* Style général pour le QTableWidget */
QTableWidget {
    /*background-color: #f8f9fa; /* Fond clair pour une apparence professionnelle */
    border: 1px solid #ced4da; /* Bordure légère et neutre */
    gridline-color: #dee2e6; /* Couleur des lignes de la grille */
    font-size: 14px; /* Taille de police standard */
    font-family: Arial, sans-serif; /* Police professionnelle */
    selection-background-color: #007bff; /* Couleur de fond pour la sélection */
    selection-color: #ffffff; /* Texte de la sélection en blanc */
}

/* Style des en-têtes horizontaux */
QHeaderView::section {
    background-color: #343a40; /* Fond sombre pour les en-têtes */
    color: #ffffff; /* Texte blanc pour le contraste */
    font-weight: bold; /* Texte en gras */
    border: 1px solid #495057; /* Lignes autour des en-têtes */
    padding: 4px; /* Espacement interne */
}

/* Style des en-têtes verticaux */
QHeaderView::section:vertical {
    background-color: #e9ecef; /* Fond clair pour les en-têtes verticaux */
    color: #495057; /* Texte gris foncé */
    border: 1px solid #dee2e6; /* Lignes autour des en-têtes verticaux */
    padding: 4px;
}

/* Style pour les lignes alternées */
QTableWidget::item {
    padding: 6px; /* Espacement interne des cellules */
}

QTableWidget::item:alternate {
    background-color: #f1f3f5; /* Fond gris clair pour les lignes alternées */
}

/* Survol des éléments */
QTableWidget::item:hover {
    background-color: #e2e6ea; /* Couleur de survol */
}

/* Style pour les bordures pendant la sélection */
QTableWidget::item:selected {
    border: 1px solid #0056b3;
}

/* Barre de défilement */
QScrollBar:vertical {
    border: none;
    background: #f8f9fa;
    width: 12px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background: #6c757d;
    border-radius: 6px;
    min-height: 20px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
    background: none;
}
